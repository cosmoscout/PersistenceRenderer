{
  "name": "persistence-renderer",
  "version": "1.0.2",
  "description": "Visualize and interact with VTK.js persistence data.",
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "webpack --mode=production",
    "build": "npm run lint:fix && npm run build:types && npm run build:js",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./src",
    "lint:fix": "eslint --ext .js,.jsx,.ts,.tsx ./src --fix"
  },
  "author": {
    "name": "Hannes K.",
    "email": "Hannes.Kruse@dlr.de"
  },
  "main": "PersistenceRenderer.js",
  "types": "index.d.ts",
  "license": "MIT",
  "dependencies": {
    "@types/nouislider": "^9.0.10",
    "core-js": "^3.18.2",
    "nouislider": "^14.7.0",
    "regenerator-runtime": "^0.13.9",
    "vtk.js": "^26.3.2"
  },
  "devDependencies": {
    "@babel/core": "7.10.3",
    "@babel/plugin-proposal-class-properties": "7.10.1",
    "@babel/plugin-proposal-object-rest-spread": "7.10.3",
    "@babel/preset-env": "7.10.3",
    "@babel/preset-typescript": "7.10.1",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "babel-loader": "8.1.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-typescript": "^8.0.2",
    "eslint-plugin-import": "^2.24.2",
    "fork-ts-checker-webpack-plugin": "5.0.5",
    "typescript": "3.9.5",
    "webpack": "5.43.0",
    "webpack-cli": "3.3.12"
  },
  "resolutions": {
    "terser": "3.14.1"
  }
}
