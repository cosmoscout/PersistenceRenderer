{
  "name": "persistence-renderer",
  "version": "1.0.1a",
  "description": "Visualize and interact with VTK.js persistence data.",
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "webpack --mode=production",
    "build": "npm run lint:fix && npm run build:types && npm run build:js",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./src",
    "lint:fix": "eslint --ext .js,.jsx,.ts,.tsx ./src --fix"
  },
  "author": {
    "name": "Hannes K.",
    "email": "Hannes.Kruse@dlr.de"
  },
  "main": "PersistenceRenderer.js",
  "types": "index.d.ts",
  "license": "MIT",
  "dependencies": {
    "@types/nouislider": "^9.0.6",
    "core-js": "^3.6.4",
    "nouislider": "^14.1.1",
    "regenerator-runtime": "^0.13.3",
    "vtk.js": "^13.7.1"
  },
  "devDependencies": {
    "@babel/core": "7.8.3",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "7.8.3",
    "@babel/preset-env": "7.8.3",
    "@babel/preset-typescript": "7.8.3",
    "@typescript-eslint/eslint-plugin": "^2.19.2",
    "babel-loader": "8.0.6",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript": "^7.0.0",
    "eslint-plugin-import": "^2.20.1",
    "fork-ts-checker-webpack-plugin": "4.0.1",
    "typescript": "3.7.5",
    "webpack": "4.41.5",
    "webpack-cli": "3.3.10"
  },
  "resolutions": {
    "terser": "3.14.1"
  }
}
